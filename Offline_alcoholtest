    let lastLogTime = 0;
    const logCooldown = 1000;

    /*******************************
     * Date/time
     *******************************/
    function updateDateTime() {
        const now = new Date();
        const options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric', hour: '2-digit', minute:'2-digit', second:'2-digit', hour12: true };
        currentDateTimeDisplay.textContent = `Current time: ${now.toLocaleDateString('en-US', options)}`;
    }
    setInterval(updateDateTime, 1000);

    /*******************************
     * Storage helpers: load/save
     *******************************/
    function loadData() {
        try {
            const rawEmployees = storage.getItem(EMPLOYEE_DATA_KEY);
            if (rawEmployees) {
                try {
                    employeeData = new Map(JSON.parse(rawEmployees));
                } catch (e) { /* use default map */ }
            }
            const rawAttendance = storage.getItem(ATTENDANCE_RECORDS_KEY);
            attendanceRecords = rawAttendance ? JSON.parse(rawAttendance) : [];
            allDepartments = [...new Set(Array.from(employeeData.values()).map(emp => emp.department))].sort();
            refreshAttendanceTable();
            countTableRows(false);
        } catch (e) { /* error handling */ }
    }

    function saveData() {
        try {
            storage.setItem(EMPLOYEE_DATA_KEY, JSON.stringify(Array.from(employeeData.entries())));
            storage.setItem(ATTENDANCE_RECORDS_KEY, JSON.stringify(attendanceRecords));
        } catch (e) { /* error handling */ }
    }

    /*******************************
     * Table & records helpers
     *******************************/
    function countTableRows(save=true) {
        totalScannedDisplay.textContent = attendanceRecords.length;
        if (save) saveData();
    }

    function addAttendanceRow(record) {
        const row = attendanceTableBody.insertRow(0);
        row.classList.add('hover:bg-gray-50');
        row.innerHTML = `
            <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">${record.employeeId}</td>
            <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">${record.name}</td>
            <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">${record.department}</td>
            <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">${record.time}</td>
        `;
    }

    function refreshAttendanceTable() {
        attendanceTableBody.innerHTML = '';
        if (!attendanceRecords || attendanceRecords.length === 0) {
            if (noRecordsMessage) noRecordsMessage.classList.remove('hidden');
        } else {
            if (noRecordsMessage) noRecordsMessage.classList.add('hidden');
            attendanceRecords.forEach(record => addAttendanceRow(record));
        }
    }

    // *** MODIFIED CORE FUNCTION ***
    function recordAttendance(employeeId, employee) {
        const now = new Date();
        const date = now.toLocaleDateString('en-US');
        const time = now.toLocaleTimeString('en-US', { hour: '2-digit', minute: '2-digit', second: '2-digit', hour12: true });

        const newRecord = {
            employeeId: employeeId,
            name: employee.name,
            department: employee.department,
            date: date,
            time: time
        };

        attendanceRecords.unshift(newRecord);
        addAttendanceRow(newRecord);
        saveData();

        displayResult(employee.name, employee.department, `${date} at ${time}`);
        
        employeeIdInput.value = '';
        employeeIdInput.focus();
        countTableRows();
    }
    
    /*******************************
     * Visual helpers
     *******************************/
    function displayResult(name, department, datetime) {
        if (!resultCard) return;
        resultCard.classList.remove('hidden');
        resultCard.classList.add('fade-in', 'border-green-500'); // Ensure border is green for success
        resultName.textContent = name;
        resultDepartment.textContent = department;
        resultDateTime.textContent = datetime;
        resultStatus.textContent = 'LOGGED';
        resultStatus.className = `text-8xl font-extrabold pulse text-green-600`;
    }

    function displayError(message) {
        if (!resultCard) return;
        resultCard.classList.remove('hidden');
        resultCard.classList.add('fade-in', 'border-red-500');
        resultName.textContent = message;
        resultDepartment.textContent = '';
        resultDateTime.textContent = new Date().toLocaleTimeString();
        resultStatus.textContent = 'ERROR';
        resultStatus.className = `text-8xl font-extrabold text-gray-500`;
        employeeIdInput.focus();
    }

    function hideSearch() {
        searchResultCard.classList.add('hidden');
    }

    /*******************************
     * Attendance input handling (Simplified)
     *******************************/
    recordAttendanceBtn.addEventListener('click', () => {
        const now = Date.now();
        if (now - lastLogTime < logCooldown) {
            console.warn("Log recorded too quickly. Ignoring duplicate.");
            return;
        }
        lastLogTime = now;

        const employeeId = employeeIdInput.value.trim();

        if (!employeeId) {
            displayError("Employee ID is required.");
            return;
        }

        const employee = employeeData.get(employeeId);

        if (employee) {
            recordAttendance(employeeId, employee);
        } else {
            displayError(`Employee ID ${employeeId} Not Found. Please upload the employee list.`);
        }
    });

    // Handle Enter keypress (for RFID/scanner input)
    employeeIdInput.addEventListener('keyup', (event) => {
        if (event.key === 'Enter' || event.keyCode === 13) {
            recordAttendanceBtn.click();
        }
    });

    /*******************************
     * Employee Search Logic (Unchanged but simplified context)
     *******************************/
    employeeSearchInput.addEventListener('input', () => {
        const query = employeeSearchInput.value.trim().toUpperCase();
        employeeSuggestions.innerHTML = '';
        employeeSuggestions.classList.add('hidden');
        searchResultCard.classList.add('hidden');

        if (query.length < 2) return;

        let foundEmployees = [];
        employeeData.forEach((employee, id) => {
            if (id.includes(query) || employee.name.includes(query)) {
                foundEmployees.push({ id, name: employee.name, department: employee.department });
            }
        });

        if (foundEmployees.length > 0) {
            foundEmployees.slice(0, 5).forEach(emp => {
                const li = document.createElement('li');
                li.className = 'autocomplete-list-item text-sm text-gray-700 hover:bg-blue-50';
                li.textContent = `${emp.id} - ${emp.name} (${emp.department})`;
                li.addEventListener('click', () => {
                    employeeIdInput.value = emp.id;
                    employeeSearchInput.value = '';
                    employeeSuggestions.classList.add('hidden');
                    // Display details in the card
                    searchResultName.textContent = emp.name;
                    searchResultId.textContent = emp.id;
                    searchResultDepartment.textContent = emp.department;
                    searchResultCard.classList.remove('hidden');
                    employeeIdInput.focus();
                });
                employeeSuggestions.appendChild(li);
            });
            employeeSuggestions.classList.remove('hidden');
        }
    });

    /*******************************
     * CSV Export Logic (Unchanged)
     *******************************/
    extractBtn.addEventListener('click', () => {
        if (attendanceRecords.length === 0) {
            alert("No attendance records to export.");
            return;
        }

        let csv = 'ID,Name,Department,Date,Time\n';
        attendanceRecords.forEach(record => {
            csv += `${record.employeeId},"${record.name}","${record.department}",${record.date},${record.time}\n`;
        });

        const blob = new Blob([csv], { type: 'text/csv;charset=utf-8;' });
        const link = document.createElement('a');
        
        const now = new Date();
        const dateStr = now.toISOString().split('T')[0];
        
        if (link.download !== undefined) { 
            link.setAttribute('href', URL.createObjectURL(blob));
            link.setAttribute('download', `AttendanceLog_${dateStr}.csv`);
            link.style.visibility = 'hidden';
            document.body.appendChild(link);
            link.click();
            document.body.removeChild(link);
        } else {
            // Fallback for browsers (like some iOS versions) that don't fully support download attribute
            alert("Please copy the following data:\n\n" + csv);
        }
    });

    /*******************************
     * Clear Log Logic (Unchanged)
     *******************************/
    resetBtn.addEventListener('click', () => {
        confirmModal.style.display = 'flex';
    });

    cancelClearBtn.addEventListener('click', () => {
        confirmModal.style.display = 'none';
    });

    confirmClearBtn.addEventListener('click', () => {
        attendanceRecords = [];
        storage.removeItem(ATTENDANCE_RECORDS_KEY);
        refreshAttendanceTable();
        countTableRows(false);
        confirmModal.style.display = 'none';
        alert("Attendance log cleared successfully.");
    });
    
    /*******************************
     * Employee Data Upload Logic (Unchanged)
     *******************************/
    employeeDataUpload.addEventListener('change', (event) => {
        const file = event.target.files[0];
        if (!file) return;

        const reader = new FileReader();
        reader.onload = (e) => {
            try {
                const data = JSON.parse(e.target.result);
                // Ensure data is an array of [ID, {name, department}]
                if (Array.isArray(data) && data.every(item => Array.isArray(item) && item.length === 2 && typeof item[0] === 'string' && typeof item[1] === 'object')) {
                    const newMap = new Map(data);
                    if (newMap.size > 0) {
                        employeeData = newMap;
                        saveData();
                        alert(`Successfully loaded ${employeeData.size} employee records!`);
                        
                        // Clear the input file value to allow re-uploading the same file
                        event.target.value = ''; 
                    } else {
                        throw new Error("Loaded file contains no valid employee records.");
                    }
                } else {
                    throw new Error("Invalid JSON/JS format. Expected an array of [ID, {name: '...', department: '...'}] entries.");
                }
            } catch (error) {
                alert(`Error loading employee data: ${error.message}. Ensure the file is valid JSON/JS.`);
            }
        };
        reader.readAsText(file);
    });

    /*******************************
     * Initialization and iOS check
     *******************************/
    function init() {
        updateDateTime();
        loadData();
        employeeIdInput.focus();
        
        if (window.location.protocol === 'file:') {
            fileProtocolOverlay.style.display = 'flex';
            continueAnywayBtn.addEventListener('click', () => {
                fileProtocolOverlay.style.display = 'none';
            });
        }
    }

    document.addEventListener('DOMContentLoaded', init);
    
    // Auto-hide search card when clicking outside
    document.addEventListener('click', (e) => {
        if (!employeeSearchInput.contains(e.target) && !employeeSuggestions.contains(e.target)) {
            employeeSuggestions.classList.add('hidden');
        }
    });
    </script>
</body>
</html>
